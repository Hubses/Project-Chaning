{
  "rules": {
    "users": {
      "$uid": {
        ".write": "$uid === auth.uid",
        ".read": "$uid === auth.uid"
      }
    },
    "projects": {
      "$uid": {
        ".write": "$uid === auth.uid",
        ".read": "$uid === auth.uid"
      }
    },
    "frameworks": {
      ".write": false,
      ".read": true
    },
    "libs": {
      ".write": false,
      ".read": true
    },
    "taskrunner": {
      ".write": false,
      ".read": true
    }
  }
}